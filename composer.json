{
    "name": "namingo/plexdns",
    "description": "A unified, multi-provider DNS management tool",
    "homepage": "https://github.com/getnamingo/plexdns",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2.0",
        "ext-pdo": "*",
        "guzzlehttp/guzzle": "^7.8",
        "vultr/vultr-php": "^1.0",
        "badcow/dns": "^4.2",
        "exonet/powerdns-php": "^4.5",
        "dnsimple/dnsimple": "^1.4",
        "namingo/bind9-api-client": "^1.0.2",
        "spatie/dns": "^2.6",
        "monolog/monolog": "^3.8",
        "vlucas/phpdotenv": "^5.6",
        "cloudflare/sdk": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "PlexDNS\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    }
}